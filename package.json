  "private": true,
  "scripts": {
    "start": "run-s build_dll_dev start_server",
    "start_server": "cross-env  NODE_ENV=development webpack-dev-server --mode development --inline --host 0.0.0.0 --devtool source-maps --progress --colors --content-base .",
    "build_dll_dev": "webpack --mode development --colors --config ./webpack.dll.config.js",
    "build_dll_prod": "webpack --mode production --colors --config ./webpack.dll.config.js",
    "prod": "cross-env  NODE_ENV=production webpack --mode production --progress --colors --display-error-details",
    "build": "run-s build_dll_prod prod"
